{
	"$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
	"rushVersion": "5.129.5",
	"yarnVersion": "1.22.19",
	"nodeSupportedVersionRange": ">=18.20.3 <19.0.0 || >=20.14.0 <22.0.0",
	"suppressNodeLtsWarning": false,

	"repository": {
		"url": "https://github.com/christophe77/custorder",
		"defaultBranch": "main",
		"defaultRemote": "origin"
	},

	"eventHooks": {
		"preRushInstall": ["common/scripts/pre-rush-install.js"],
		"postRushInstall": [],
		"preRushBuild": [],
		"postRushBuild": [],
		"preRushx": [],
		"postRushx": []
	},

	"projects": [
		{
			"packageName": "front",
			"projectFolder": "tools/front",
			"reviewCategory": "tools",
			"tags": ["frontend-team"]
		},

		{
			"packageName": "back",
			"projectFolder": "tools/back",
			"reviewCategory": "tools",
			"tags": ["backend-team"]
		}
	]
}
